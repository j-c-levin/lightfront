      "eventsFunctions": [
        {
          "description": "Update the charge of each planet connected to a start",
          "fullName": "Run charge tick",
          "functionType": "Action",
          "name": "charge_tick",
          "sentence": "Run charge tick",
          "events": [
            {
              "colorB": 228,
              "colorG": 176,
              "colorR": 74,
              "creationTime": 0,
              "disabled": false,
              "folded": false,
              "name": "Charge tick",
              "source": "",
              "type": "BuiltinCommonInstructions::Group",
              "events": [
                {
                  "disabled": false,
                  "folded": false,
                  "type": "BuiltinCommonInstructions::JsCode",
                  "inlineCode": "const stars = runtimeScene.getObjects(\"star\")\nconst links = runtimeScene.linkedObjectsManager\n\nstars.forEach(star => {\n    chargeNetwork(star)\n})\n\nfunction chargeNetwork(star) {\n    const roots = links.getObjectsLinkedWith(star)\n    roots.forEach(root => {\n        const planets = search(root)\n        planets.forEach(chargePlanet)\n    })\n}\n\n// Breadth-first search over all planets connected to the star\nfunction search(root) {\n    const toSearch = [root]\n    const discovered = [root]\n\n    while (toSearch.length > 0) {\n        const current = toSearch.shift()\n        let planets = links.getObjectsLinkedWith(current).filter(planet => removeDiscovered(planet, discovered))\n        toSearch.push(...planets)\n        discovered.push(...planets)\n    }\n\n    return discovered\n}\n\nfunction removeDiscovered(obj, discovered) {\n    return discovered.indexOf(obj) === -1 && obj.name !== \"star\"\n}\n\nfunction chargePlanet(planet, index, array) {\n    // weight the charge towards the closer planets in the network\n    const baseCharge = 2 / array.length\n    const ratio = (array.length === 1) ? 1 : 1.5 - (1 / (array.length - 1)) * index\n    const chargeRate = baseCharge * ratio\n\n    const current = planet.getVariables().get(\"charge\").getAsNumber()\n    const max = planet.getVariables().get(\"charge_max\").getAsNumber()\n\n    const charge = Math.min(current + chargeRate, max)\n    \n    planet.getVariables().get(\"charge\").setNumber(charge)\n}",
                  "parameterObjects": "",
                  "useStrict": true
                }
              ],
              "parameters": []
            }
          ],
          "parameters": [],
          "objectGroups": []
        },
