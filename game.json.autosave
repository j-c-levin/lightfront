            {
              "description": "Sets up the home networks",
              "fullName": "Setup game",
              "functionType": "Action",
              "name": "setup_game",
              "sentence": "Setup game _PARAM0_",
              "events": [
                {
                  "colorB": 228,
                  "colorG": 176,
                  "colorR": 74,
                  "creationTime": 0,
                  "disabled": false,
                  "folded": false,
                  "name": "Setup home networks",
                  "source": "",
                  "type": "BuiltinCommonInstructions::Group",
                  "events": [
                    {
                      "disabled": false,
                      "folded": false,
                      "type": "BuiltinCommonInstructions::JsCode",
                      "inlineCode": "const star = runtimeScene.getObjects(\"star\").find((star) => {\n    return star.getVariables().get(\"is_home\").getAsString() === \"true\"\n})\n\nconst planet = runtimeScene.getObjects(\"planet\").find((planet) => {\n    return planet.getVariables().get(\"is_home\").getAsString() === \"true\"\n})\n\nconst links = {}\nlinks[star.id] = [planet]\nlinks[planet.id] = [star]\nobjects[0].getBehavior(\"controller\")._behaviorData[\"links\"] = links",
                      "parameterObjects": "Object",
                      "useStrict": true
                    }
                  ],
                  "parameters": []
                }
              ],
              "parameters": [
                {
                  "codeOnly": false,
                  "defaultValue": "",
                  "description": "Object",
